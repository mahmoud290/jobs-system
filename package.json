{
  "name": "jobs-project",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "start:test": "cross-env NODE_ENV=test npm run start",
    "test:test": "cross-env NODE_ENV=test npm run test",

    "test:users": "cross-env NODE_ENV=test jest src/users",
    "test:jobs": "cross-env NODE_ENV=test jest src/jobs",
    "test:mailer": "cross-env NODE_ENV=test jest src/mailer",
    "test:notifications": "cross-env NODE_ENV=test jest src/notifications",

    "test:jobs:unit": "cross-env NODE_ENV=test jest --testPathPattern=jobs.*\\.unit\\.spec\\.ts$",
    "test:jobs:int": "cross-env NODE_ENV=test jest --testPathPattern=jobs.*\\.int\\.spec\\.ts$",
    "test:jobs:e2e": "cross-env NODE_ENV=test jest --testPathPattern=jobs.*\\.e2e\\.spec\\.ts$",

    "test:users:unit": "cross-env NODE_ENV=test jest --testPathPattern=users.*\\.unit\\.spec\\.ts$",
    "test:users:int": "cross-env NODE_ENV=test jest --testPathPattern=users.*\\.int\\.spec\\.ts$",
    "test:users:e2e": "cross-env NODE_ENV=test jest --testPathPattern=users.*\\.e2e\\.spec\\.ts$",

    "test:mailer:unit": "cross-env NODE_ENV=test jest --testPathPattern=mailer.*\\.unit\\.spec\\.ts$",
    "test:mailer:int": "cross-env NODE_ENV=test jest --testPathPattern=mailer.*\\.int\\.spec\\.ts$",
    "test:mailer:e2e": "cross-env NODE_ENV=test jest --testPathPattern=mailer.*\\.e2e\\.spec\\.ts$",

    "test:notifications:unit": "cross-env NODE_ENV=test jest --testPathPattern=notifications.*\\.unit\\.spec\\.ts$",
    "test:notifications:int": "cross-env NODE_ENV=test jest --testPathPattern=notifications.*\\.int\\.spec\\.ts$",
    "test:notifications:e2e": "cross-env NODE_ENV=test jest --testPathPattern=notifications.*\\.e2e\\.spec\\.ts$"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.3",
    "@nestjs/core": "^11.1.3",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/typeorm": "^11.0.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^16.5.0",
    "nodemailer": "^7.0.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.32",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
}
